{
    "name": "protoflex",
    "version": "0.1.1",
    "description": "Arbitrary Protobuf (de-)serializing library",
    "author": "teidesu",
    "license": "MIT",
    "main": "src/index.js",
    "scripts": {
        "build": "npm run test && npm run build:lib && npm run build:doc && npm run build:repl",
        "test": "mocha test/**/*.spec.js",
        "build:lib": "webpack",
        "build:doc": "typedoc --out docs/api --includeDeclarations --excludeExternals --readme API.md --name protoflex src/index.d.ts src/utils.d.ts && touch ./docs/.nojekyll",
        "build:repl": "npm run build --prefix repl"
    },
    "files": [
        "src/",
        "dist",
        "utils.js",
        "utils.d.ts"
    ],
    "dependencies": {
        "long": "^4.0.0",
        "@types/long": "^4.0.0"
    },
    "devDependencies": {
        "@babel/core": "^7.7.5",
        "@babel/preset-env": "^7.7.5",
        "@types/chai": "^4.2.5",
        "@types/mocha": "^5.2.7",
        "@types/node": "^12.12.14",
        "babel-loader": "^8.0.6",
        "chai": "^4.2.0",
        "mocha": "^6.2.2",
        "terser-webpack-plugin": "^2.2.2",
        "typedoc": "^0.15.3",
        "webpack": "^4.41.2",
        "webpack-cli": "^3.3.10"
    },
    "browserslist": [
        "> 1%",
        "not dead"
    ]
}
